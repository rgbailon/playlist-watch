{
  "builds": [
    {
      "src": "deftones_search/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "pythonVersion": "3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "deftones_search/wsgi.py"
    }
  ],
  "installCommand": "pip install --break-system-packages -r requirements.txt",
  "buildCommand": "python manage.py migrate --noinput && python manage.py collectstatic --noinput",
  "outputDirectory": "staticfiles",
